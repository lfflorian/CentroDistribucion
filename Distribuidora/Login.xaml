<Window x:Class="Distribuidora.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Distribuidora"
        mc:Ignorable="d"
        Title="Login" Height="300" Width="300">
    <Grid>
        <Label Content="Usuario" HorizontalAlignment="Left" Margin="100,48,0,0" VerticalAlignment="Top"/>
        <Label Content="Contraseña" HorizontalAlignment="Left" Margin="100,111,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.764,0.812"/>
        <TextBox Name="txtUsuario" HorizontalAlignment="Left" Height="23" Margin="83,79,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.47,-0.057"/>
        <TextBox Name="txtContraseña" HorizontalAlignment="Left" Height="23" Margin="83,142,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.47,-0.057"/>
        <Button Content="Ingresar" HorizontalAlignment="Left" Margin="100,183,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
